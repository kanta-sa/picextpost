<h1>登録済みのユーザー一覧</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>登録日</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
    	  <td><%= link_to user.name, administrator_user_path(user) %></td>
    	  <td><%= user.created_at.to_s(:datetime_jp) %></td>
      </tr>
    <% end %>
  </tbody>
</table>