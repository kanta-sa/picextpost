<% if users.any? %>
  <ul class="list-unstyled">
    <% users.each do |user| %>
      <% unless user.admin %>
      <li class="media">
        <% if user.image? %>
          <%= image_tag user.image.to_s, :size => '80x80', class: 'mypage-article-img' %>
        <% else %>
          <%= image_tag 'human.jpg', :size => '80x80', class: 'mypage-article-img' %>
        <% end %>
        <div class="media-body">
          <div>
            <%= user.name %>
          </div>
          <div>
            <p><%= link_to 'View profile', user %></p>
          </div>
        </div>
      </li>
      <% end %>
    <% end %>
  </ul> 
<% end %>