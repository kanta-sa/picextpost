<% if users.any? %>
  <ul class="list-unstyled">
    <% users.each do |user| %>
      <% unless user.admin %>
      <li class="media">
        <img class="mr-2 rounded" src="app/assets/images/human.jpg" alt="人間">
        <div class="media-body">
          <div>
            <%= user.name %>
          </div>
          <div>
            <p><%= link_to 'View profile', user_path(user) %></p>
          </div>
        </div>
      </li>
      <% end %>
    <% end %>
  </ul> 
  <%= paginate users %>
<% end %>